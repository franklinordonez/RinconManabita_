<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RinconManabita.MainPage">
    <StackLayout>
        <Button Command="{Binding LocalizameCommand}" Text="Localizame"></Button>
        <Label Text="Longitud"></Label>
        <Entry Text="{Binding Longitud}"></Entry>
        <Label Text="Latitud"></Label>
        <Entry Text="{Binding Latitud}"></Entry>
        <Label Text="{Binding Error}" TextColor="Red"></Label>
    </StackLayout>
</ContentPage>